{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://*:5001",
        "Protocols": "Http2"
      },
      "WebApi": {
        "Url": "http://*:80",
        "Protocols": "Http1AndHttp2"
      }
    }
  }
}